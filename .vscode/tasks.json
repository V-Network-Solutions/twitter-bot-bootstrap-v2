{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "setup",
      "command": "npm run dev-setup",
      "type": "shell"
    },
    {
      "label": "install",
      "command": "npm i",
      "type": "shell"
    },
    {
      "label": "format",
      "command": "npm run format",
      "type": "shell"
    },
    {
      "label": "build",
      "command": "npm run build",
      "type": "shell",
      "problemMatcher": "$tsc",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "test",
      "command": "npm test",
      "type": "shell",
      "problemMatcher": "$tsc",
      "group": {
        "kind": "test",
        "isDefault": true
      }
    }
  ]
}
