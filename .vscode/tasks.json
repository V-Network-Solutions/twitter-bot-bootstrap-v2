{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Update Contributors",
      "command": "npm run contrib",
      "type": "shell"
    },
    {
      "label": "Fix Line Formatting",
      "command": "npm run format",
      "type": "shell"
    },
    {
      "label": "Generate Docs",
      "command": "npm run gen-docs",
      "type": "shell"
    },
    {
      "label": "Install Dependencies",
      "command": "npm i",
      "type": "shell"
    },
    {
      "label": "Setup Dev Enviornment",
      "command": "npm run dev-setup",
      "type": "shell"
    },
    {
      "label": "Start Bot",
      "command": "npm run format && npm start",
      "type": "shell",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
