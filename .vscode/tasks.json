{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "contribute",
      "command": "npm run contrib",
      "type": "shell"
    },
    {
      "label": "format",
      "command": "npm run format",
      "type": "shell"
    },
    {
      "label": "install",
      "command": "npm i",
      "type": "shell"
    },
    {
      "label": "setup",
      "command": "npm run dev-setup",
      "type": "shell"
    },
    {
      "label": "start",
      "command": "npm run format && npm start",
      "type": "shell",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
