{
  "name": "twitter-bot-bootstrap-v2",
  "version": "0.3.3",
  "description": "A bootstrap for a twitter bot! A project carried on by V-Network Solutions",
  "main": "index.js",
  "scripts": {
    "add-express": "npx express-generator twitterBot -â€“git --view pug && cd twitterBot && npm i && cd ..",
    "contrib": "contributor",
    "dev-setup": "npm i && npm run new-env && npm run add-express",
    "format": "pretty-quick",
    "new-env": "node -e \"const path = require('path'); const srcFile = __dirname + '/.sample/.env.sample'; const envFile = __dirname+'/.env';  require('fs').copyFile(srcFile, envFile, function(err) { if (err) { console.log(err); console.log('Environment configuration file creation failed!!'); } else { console.log('Environment configuration file successfully created!') }})\"",
    "precommit": "pretty-quick --staged",
    "setup": "npm run dev-setup",
    "start": "node index.js"
  },
  "pre-commit": [
    "format"
  ],
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/unique-random-array": "^2.0.1",
