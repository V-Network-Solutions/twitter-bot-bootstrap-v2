{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 10,
    "source": {
        "include": ["./src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    }
}